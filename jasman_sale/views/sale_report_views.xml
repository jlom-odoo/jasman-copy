<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="sale_report_search_view" model="ir.ui.view">
        <field name="name">sale.report.search.inherit.jasman</field>
        <field name="model">sale.report</field>
        <field name="inherit_id" ref="sale.view_order_product_search"/>
        <field name="arch" type="xml">
            <xpath expr="//filter[@name='User']" position="after">
                <filter string="Technician" name="technician_id" context="{'group_by':'technician_id'}"/>
            </xpath>
        </field>
    </record>
</odoo>
