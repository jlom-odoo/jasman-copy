<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="crm_team_view_form" model="ir.ui.view">
        <field name="name">crm.team.form.inherit.jasman</field>
        <field name="model">crm.team</field>
        <field name="inherit_id" ref="sales_team.crm_team_view_form"/>
        <field name="arch" type="xml">
            <xpath expr="//group[@name='left']" position="after">
                <group name="right" string="Sale Presets">
                    <field name="is_vehicle_required"/>
                    <field name="property_account_position_id"/>
                    <field name="journal_id"/>
                    <field name="insurance_company"/>
                    <field name="policy_number"/>
                </group>
            </xpath>
        </field>
    </record>
</odoo>
