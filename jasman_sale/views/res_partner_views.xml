<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="res_partner_view_form" model="ir.ui.view">
        <field name="name">res.partner.form.inherit.jasman</field>
        <field name="model">res.partner</field>
        <field name="inherit_id" ref="base.view_partner_form"/>
        <field name="arch" type="xml">
            <xpath expr="//notebook" position="inside">
                <page name="sale_documents" string="Documentation">
                    <group string="Sale Documents">
                        <field name="has_doc_authorization"/>
                        <field name="has_doc_survey"/>
                        <field name="has_doc_identification"/>
                        <field name="has_doc_circulation_card"/>
                        <field name="has_doc_picture_evidence"/>
                        <field name="has_doc_signed_invoice"/>
                    </group>
                </page>
            </xpath>
        </field>
    </record>
</odoo>
