<?xml version='1.0' encoding='utf-8'?>
<odoo>
  <record id="res_users_view_form_inherit_jasman" model="ir.ui.view">
    <field name="name">res.users.view.form.inherit.jasman</field>
    <field name="model">res.users</field>
    <field name="inherit_id" ref="base.view_users_form"/>
    <field name="arch" type="xml">
      <xpath expr="//page[@name='preferences']" position="after">
        <page name="dimensions" string="Dimensions">
          <group>
              <field name="ceco_analytic_account_id"/>
              <field name="zona_analytic_account_id"/>
              <field name="subzona_analytic_account_id"/>
          </group>
        </page>
      </xpath>
    </field>
  </record>
</odoo>
