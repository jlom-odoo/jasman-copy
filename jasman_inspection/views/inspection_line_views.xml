<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <record id="inspection_line_form" model="ir.ui.view">
        <field name="name">inspection.line.form</field>
        <field name="model">inspection.line</field>
        <field name="priority" eval="1"/>
        <field name="arch" type="xml">
            <form string="Stage">
                <sheet>
                    <div class="oe_title">
                        <label for="name"/>
                        <h1>
                            <field name="name"/>
                        </h1>
                    </div>
                    <group>
                        <group>
                            <field name="sale_order_id" readonly="1"/>
                            <field name="quotation_template_id"/>
                            <field name="task_id"/>
                            <field name="urgency" widget="priority"/>
                        </group>
                    </group>
                </sheet>
            </form>
        </field>
    </record>
</odoo>
